<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane maxWidth="450.0" minWidth="450.0" prefWidth="450.0" style="-fx-border-radius: 15; -fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.viewcontroller.FullDayViewController">
   <children>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="450.0" style="-fx-background-color: #FFFFFF;">
         <children>
            <VBox prefHeight="400.0" prefWidth="450.0">
               <children>
                  <HBox prefWidth="450.0" style="-fx-border-color: #BDBDBD; -fx-border-width: 1;">
                     <children>
                        <Button fx:id="backBtn" mnemonicParsing="false" onMouseClicked="#closeFullDay" style="-fx-background-color: #FFFFFF; -fx-border-radius: 15;" styleClass="sidebutton" stylesheets="@../styles.css">

                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </Button>
                        <Label text="Full Day Planning">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="7.0" />
                           </HBox.margin>
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox fx:id="dayPageBox" prefHeight="356.0" prefWidth="450.0" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
