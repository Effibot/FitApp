<AnchorPane fx:id="main" prefHeight="620.0" prefWidth="900.0" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.viewcontroller.MainViewController">
	<children>
		<BorderPane fx:id="container" prefHeight="600.0" prefWidth="900.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<top>
            <VBox fx:id="topBox">
               <children>
                  <StackPane>
                     <children>
                        <Pane styleClass="container" stylesheets="@../styles.css" />
            				<HBox fx:id="topMenu" alignment="CENTER" prefHeight="17.0" prefWidth="900.0" stylesheets="@../styles.css">
            					<children>
            					 <ImageView fitWidth="40.0" layoutX="67.0" layoutY="156.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../icons/fitApp_LogIn.png" />
                                 </image>
                              </ImageView>
            						<Label layoutX="131.0" layoutY="115.0" prefHeight="52.0" prefWidth="93.0" text="FitApp" textFill="#1b3039" wrapText="true">
            							<font>
            								<Font size="17.0" />
            							</font>
            						</Label>
            						<Pane prefHeight="29.0" prefWidth="802.0" />
                              <StackPane>
                                 <children>
                  						<Circle accessibleRole="BUTTON" fill="#43a047" radius="8.0" stroke="#5c7079" strokeType="INSIDE" strokeWidth="2.0" />
                                    <ImageView fx:id="btnReduce" fitHeight="15.0" fitWidth="42.0" onMouseClicked="#onMouseClickedEvent" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../icons/minimize.png" />
                                       </image>
                                    </ImageView>
                                 </children>
                              </StackPane>
                              <StackPane>
                                 <children>
                  						<Circle accessibleRole="BUTTON" fill="#d04f25" radius="8.0" stroke="#5c7079" strokeType="INSIDE" strokeWidth="2.0">
